{
  "name": "@acronis-platform/ui-component-library",
  "type": "module",
  "version": "1.35.3",
  "repository": "https://github.com/acronis/ui-component-library",
  "bugs": {
    "url": "https://github.com/acronis/ui-component-library/issues"
  },
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js",
    "./components/*": "./dist/components/*",
    "./styles/*": "./dist/styles/*",
    "./directives/*": "./dist/directives/*"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "CHANGELOG.md",
    "dist",
    "types.d.ts"
  ],
  "scripts": {
    "dev": "NODE_ENV=dev UI_BUILD=true vite build --watch",
    "dev:component": "tsx scripts/serve.ts",
    "build": "UI_BUILD=true vite build",
    "build:report": "VITE_REPORT=true npm run build",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "test:update": "vitest --update",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --config video=false --component --browser chrome --config-file cypress.config.ts",
    "cypress:update": "pnpm run cypress:run --env updateSnapshots=true",
    "cypress:docker:build": "docker build --tag 'cypress-tests' -f ../../Dockerfile.cypress ../..",
    "cypress:docker:run": "docker run --name 'vz-regression' --rm -v ./cypress:/workspace/packages/ui/cypress 'cypress-tests' --component --browser chrome  --env updateSnapshots=true",
    "lint": "npm-run-all --aggregate-output -c -p lint:code lint:ts lint:stylelint",
    "lint:stylelint": "stylelint \"**/*.{css,vue}\"",
    "lint:stylelint:fix": "pnpm run lint:stylelint -- --fix",
    "lint:code": "eslint . --quiet",
    "lint:code:fix": "pnpm run lint:code --fix",
    "lint:ts": "vue-tsc --noemit",
    "lint:fix": "npm-run-all -c \"lint:code:fix\" \"lint:stylelint:fix\" || exit 0",
    "storybook:build": "storybook build",
    "storybook:dev": "storybook dev -p 6006"
  },
  "peerDependencies": {
    "@acronis-platform/icons": "workspace:*",
    "vee-validate": "^4.9",
    "vue": "^3.5.6",
    "vue-router": "^4.4.4"
  },
  "peerDependenciesMeta": {
    "vue-router": {
      "optional": true
    }
  },
  "dependencies": {
    "@acronis-platform/ui-syntax": "workspace:*",
    "@antfu/utils": "9.3.0",
    "@floating-ui/vue": "1.1.9",
    "@vee-validate/rules": "4.15.1",
    "@vueuse/components": "14.1.0",
    "@vueuse/core": "14.1.0",
    "@vueuse/integrations": "14.1.0",
    "chart.js": "4.5.1",
    "chartjs-adapter-date-fns": "3.0.0",
    "chartjs-plugin-annotation": "3.1.0",
    "colord": "2.9.3",
    "date-fns": "4.1.0",
    "deepmerge": "4.3.1",
    "focus-trap": "7.7.1",
    "highlight.js": "11.11.1",
    "lodash-es": "4.17.22",
    "md-editor-v3": "6.3.1",
    "perfect-scrollbar": "1.5.6",
    "sortablejs": "1.15.6",
    "tiny-emitter": "2.1.0",
    "vue-i18n": "11.2.8"
  },
  "devDependencies": {
    "@acronis-platform/browserslist-config": "workspace:*",
    "@acronis-platform/configs": "workspace:*",
    "@acronis-platform/utils": "workspace:*",
    "@cypress/vue": "6.0.2",
    "@simonsmith/cypress-image-snapshot": "10.0.3",
    "@storybook/addon-designs": "11.1.1",
    "@storybook/addon-essentials": "8.6.14",
    "@storybook/blocks": "8.6.14",
    "@storybook/manager-api": "8.6.14",
    "@storybook/preview-api": "8.6.14",
    "@storybook/theming": "8.6.14",
    "@storybook/vue3": "10.1.11",
    "@storybook/vue3-vite": "10.1.11",
    "@types/lodash-es": "4.17.12",
    "@types/node": "25.0.3",
    "@types/prompts": "2.4.9",
    "@vitejs/plugin-vue": "6.0.3",
    "@vitest/coverage-v8": "4.0.16",
    "@vitest/ui": "4.0.16",
    "@vitest/utils": "4.0.16",
    "@vue/test-utils": "2.4.6",
    "browserslist": "4.28.1",
    "cypress": "15.8.1",
    "eslint": "9.39.2",
    "eslint-plugin-cypress": "5.2.0",
    "eslint-plugin-vuejs-accessibility": "2.4.1",
    "execa": "9.6.1",
    "jsdom": "27.4.0",
    "kolorist": "1.8.0",
    "minimist": "1.2.8",
    "npm-run-all": "4.1.5",
    "postcss": "8.5.6",
    "postcss-custom-media": "11.0.6",
    "postcss-import": "16.1.1",
    "postcss-load-config": "6.0.1",
    "postcss-mixins": "12.1.2",
    "postcss-preset-env": "10.6.0",
    "prettier": "3.7.4",
    "prompts": "2.4.2",
    "rollup-plugin-node-externals": "8.1.2",
    "rollup-plugin-visualizer": "6.0.5",
    "sass": "1.97.2",
    "storybook": "10.1.11",
    "storybook-vue3-router": "7.0.0",
    "stylelint": "16.26.1",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vite": "7.3.0",
    "vite-plugin-dts": "4.5.4",
    "vite-plugin-lib-inject-css": "2.2.2",
    "vitest": "4.0.16",
    "vitest-axe": "0.1.0",
    "vitest-canvas-mock": "1.1.3",
    "vue": "3.5.26",
    "vue-router": "4.6.4",
    "vue-tsc": "3.2.2"
  },
  "browserslist": [
    "extends @acronis-platform/browserslist-config"
  ]
}
