{
  "name": "@acronis-platform/ui-component-library",
  "type": "module",
  "version": "1.32.0",
  "repository": "https://github.com/acronis/ui-component-library",
  "bugs": {
    "url": "https://github.com/acronis/ui-component-library/issues"
  },
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js",
    "./components/*": "./dist/components/*",
    "./styles/*": "./dist/styles/*",
    "./directives/*": "./dist/directives/*"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "CHANGELOG.md",
    "dist",
    "types.d.ts"
  ],
  "scripts": {
    "dev": "NODE_ENV=dev UI_BUILD=true vite build --watch",
    "dev:component": "tsx scripts/serve.ts",
    "build": "UI_BUILD=true vite build",
    "build:report": "VITE_REPORT=true npm run build",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "test:update": "vitest --update",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --config video=false --component --browser chrome --config-file cypress.config.ts",
    "cypress:update": "pnpm run cypress:run --env updateSnapshots=true",
    "cypress:docker:build": "docker build --tag 'cypress-tests' -f ../../Dockerfile.cypress ../..",
    "cypress:docker:run": "docker run --name 'vz-regression' --rm -v ./cypress:/workspace/packages/ui/cypress 'cypress-tests' --component --browser chrome  --env updateSnapshots=true",
    "lint": "npm-run-all --aggregate-output -c -p lint:code lint:ts lint:stylelint",
    "lint:stylelint": "stylelint \"**/*.{css,vue}\"",
    "lint:stylelint:fix": "pnpm run lint:stylelint -- --fix",
    "lint:code": "eslint . --quiet",
    "lint:code:fix": "pnpm run lint:code --fix",
    "lint:ts": "vue-tsc --noemit",
    "lint:fix": "npm-run-all -c \"lint:code:fix\" \"lint:stylelint:fix\" || exit 0",
    "storybook:build": "storybook build",
    "storybook:dev": "storybook dev -p 6006"
  },
  "peerDependencies": {
    "@acronis-platform/icons": "workspace:*",
    "vee-validate": "^4.9",
    "vue": "^3.5.6",
    "vue-router": "^4.4.4"
  },
  "peerDependenciesMeta": {
    "vue-router": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/utils": "0.7.8",
    "@floating-ui/vue": "1.0.6",
    "@vee-validate/rules": "4.9.6",
    "@vueuse/components": "10.11.0",
    "@vueuse/core": "10.9.0",
    "@vueuse/integrations": "10.11.0",
    "chart.js": "4.4.3",
    "chartjs-adapter-date-fns": "3.0.0",
    "chartjs-plugin-annotation": "3.0.1",
    "colord": "2.9.3",
    "date-fns": "3.6.0",
    "deepmerge": "4.3.1",
    "focus-trap": "7.6.1",
    "highlight.js": "11.9.0",
    "lodash-es": "4.17.21",
    "md-editor-v3": "4.17.3",
    "perfect-scrollbar": "1.5.5",
    "sortablejs": "1.15.2",
    "vue-i18n": "10.0.4"
  },
  "devDependencies": {
    "@acronis-platform/browserslist-config": "workspace:*",
    "@acronis-platform/configs": "workspace:*",
    "@acronis-platform/utils": "workspace:*",
    "@cypress/vue": "6.0.1",
    "@simonsmith/cypress-image-snapshot": "9.0.3",
    "@storybook/addon-designs": "8.0.3",
    "@storybook/addon-essentials": "8.2.9",
    "@storybook/blocks": "8.2.9",
    "@storybook/manager-api": "8.2.9",
    "@storybook/preview-api": "8.2.9",
    "@storybook/theming": "8.2.9",
    "@storybook/vue3": "8.2.9",
    "@storybook/vue3-vite": "8.2.9",
    "@types/lodash-es": "4.17.12",
    "@types/node": "20.11.30",
    "@types/prompts": "2.4.9",
    "@vitejs/plugin-vue": "5.1.3",
    "@vitest/coverage-v8": "2.1.2",
    "@vitest/ui": "2.1.2",
    "@vitest/utils": "2.1.2",
    "@vue/test-utils": "2.4.6",
    "browserslist": "4.23.0",
    "cypress": "13.12.0",
    "eslint": "9.12.0",
    "eslint-plugin-cypress": "3.5.0",
    "eslint-plugin-vuejs-accessibility": "2.4.1",
    "execa": "9.1.0",
    "jsdom": "24.0.0",
    "kolorist": "1.8.0",
    "minimist": "1.2.8",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.38",
    "postcss-custom-media": "10.0.4",
    "postcss-import": "16.1.0",
    "postcss-load-config": "5.0.3",
    "postcss-mixins": "10.0.0",
    "postcss-preset-env": "9.5.2",
    "prettier": "3.2.5",
    "prompts": "2.4.2",
    "rollup-plugin-node-externals": "7.0.1",
    "rollup-plugin-visualizer": "5.12.0",
    "sass": "1.77.4",
    "storybook": "8.2.9",
    "storybook-vue3-router": "5.0.0",
    "stylelint": "16.2.1",
    "tsx": "4.7.2",
    "typescript": "5.5.3",
    "vite": "5.4.6",
    "vite-plugin-dts": "3.8.3",
    "vite-plugin-lib-inject-css": "2.0.1",
    "vitest": "2.1.2",
    "vitest-axe": "0.1.0",
    "vitest-canvas-mock": "0.3.3",
    "vue": "3.5.6",
    "vue-router": "4.4.4",
    "vue-tsc": "2.1.6"
  },
  "browserslist": [
    "extends @acronis-platform/browserslist-config"
  ]
}
