{
  "name": "@acronis-platform/icons",
  "type": "module",
  "version": "0.0.1",
  "description": "Include SVG icon collections with Vue 3 examples.",
  "author": "Acronis",
  "license": "MIT",
  "keywords": [
    "icons",
    "svg",
    "vue",
    "vue3",
    "ui-kit"
  ],
  "sideEffects": false,
  "exports": {
    "./*.js": {
      "import": "./public/*.js"
    },
    "./*.svg": {
      "import": "./public/svg/*.svg"
    },
    "./*": {
      "import": "./public/*.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "public",
    "vue"
  ],
  "scripts": {
    "build-and-publish": "pnpm publish-to-registry",
    "publish-to-registry": "pnpm publish --no-git-checks",
    "figma-icons": "node node_modules/@acronis-platform/figma-fetcher"
  },
  "peerDependencies": {
    "vue": "3.4.30"
  },
  "devDependencies": {
    "@acronis-platform/figma-fetcher": "workspace:*"
  }
}
