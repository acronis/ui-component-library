{
  "color": {
    "$type": "color",
    "tertiary": {
      "base": {
        "$value": "{color.brand.tertiary.value}"
      },
      "darker": {
        "$value": "hsl(from var(--acv-color-brand-tertiary) calc(h + 1) calc(s - 6) calc(l - 25))"
      },
      "dark": {
        "$value": "hsl(from var(--acv-color-brand-tertiary) calc(h + 1) calc(s - 4) calc(l - 20))"
      },
      "light": {
        "$value": "hsl(from var(--acv-color-tertiary) calc(h - 1) calc(s + 2) calc(l + 18))"
      },
      "lighter": {
        "$value": "hsl(from var(--acv-color-tertiary) calc(h - 2) calc(s + 4) calc(l + 36))"
      },
      "lightest": {
        "$value": "hsl(from var(--acv-color-tertiary) calc(h - 3) calc(s + 5) calc(l + 49))"
      }
    }
  }
}

