{
  "color": {
    "$type": "color",
    "disabled": {
      "surface": {
        "value": "{color.button.disabled}",
        "type": "color"
      },
      "border": {
        "value": "{color.secondary.muted}",
        "type": "color"
      },
      "text": {
        "value": "{color.text.disabled}",
        "type": "color"
      }
    },
    "accent": {
      "surface": {
        "value": "{color.button.active.secondary}",
        "type": "color"
      }
    },
    "active": {
      "value": "{color.button.active.primary}",
      "type": "color"
    },
    "focus": {
      "value": "{color.button.focus}",
      "type": "color"
    },
    "highlight": {
      "value": "{color.text.highlight.result}",
      "type": "color"
    },
    "hover": {
      "value": "{color.button.hover.primary}",
      "type": "color"
    }
  }
}

