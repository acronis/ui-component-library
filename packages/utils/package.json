{
  "name": "@acronis-platform/utils",
  "type": "module",
  "version": "0.1.1",
  "description": "Includes UI component library internal utilities",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "lint": "npm-run-all --aggregate-output -c -p lint:code lint:ts",
    "lint:fix": "npm-run-all -c \"lint:code --fix\" lint:ts || exit 0",
    "lint:code": "eslint .",
    "lint:ts": "tsc --noemit",
    "test": "vitest",
    "test:cover": "vitest run --coverage",
    "test:dev": "vitest dev"
  },
  "dependencies": {
    "lodash-es": "4.17.22",
    "semver": "7.7.3"
  },
  "devDependencies": {
    "@acronis-platform/configs": "workspace:*",
    "@pnpm/types": "1001.2.0",
    "@types/lodash-es": "4.17.12",
    "@vitest/coverage-v8": "4.0.16",
    "eslint": "9.39.2",
    "npm-run-all": "4.1.5",
    "rollup-plugin-node-externals": "8.1.2",
    "typescript": "5.9.3",
    "vite": "7.3.0",
    "vite-plugin-dts": "4.5.4",
    "vitest": "4.0.16"
  }
}
