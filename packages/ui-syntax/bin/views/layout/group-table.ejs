<table class="group__table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Value</th>
      <th>Example</th>
    </tr>
  </thead>
  <tbody>
    <% group.tokens.forEach((token) => { %>
      <tr>
        <td><%- include(`/name/${group.nameFormat ?? 'default'}`, { token }) %></td>
        <td><%- include(`/value/${group.valueFormat ?? 'default'}`, { token }) %></td>
        <td><%- include(`/sample/${group.sampleFormat ?? 'empty'}`, { token }) %></td>
      </tr>
    <% }) %>
  </tbody>
</table>
